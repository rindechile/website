{
  "name": "transparenta-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run generate:mapping && next build",
    "start": "next start",
    "lint": "eslint",
    "generate:mapping": "tsx scripts/generate-municipality-mapping.ts",
    "preinstall": "npx only-allow pnpm",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:dev": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed-unspsc.ts",
    "db:seed:remote": "tsx scripts/seed-unspsc.ts --remote",
    "db:seed:data": "tsx scripts/seed-data-tables.ts",
    "db:seed:data:remote": "tsx scripts/seed-data-tables.ts --remote",
    "db:seed:purchases": "tsx scripts/seed-purchases-optimized.ts",
    "db:seed:purchases:remote": "tsx scripts/seed-purchases-optimized.ts --remote"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.13.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3": "^7.9.0",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "lucide-react": "^0.554.0",
    "next": "16.0.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/d3": "^7.4.3",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wrangler": "^4.50.0"
  }
}
