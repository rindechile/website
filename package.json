{
  "name": "rindechile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cloudflare": "opennextjs-cloudflare build",
    "start": "next start",
    "lint": "eslint",
    "generate:mapping": "tsx scripts/generate-municipality-mapping.ts",
    "generate:static-data": "tsx scripts/generate-static-data.ts",
    "preinstall": "npx only-allow pnpm",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:dev": "drizzle-kit studio",
    "db:drop:all": "wrangler d1 execute transparenta --local --file=scripts/sql/drop-all-tables.sql",
    "db:drop:all:remote": "wrangler d1 execute transparenta --remote --file=scripts/sql/drop-all-tables.sql",
    "db:reset": "pnpm run db:drop:all && pnpm run drizzle:migrate",
    "db:reset:remote": "pnpm run db:drop:all:remote && pnpm run drizzle:migrate",
    "db:seed:unspsc": "tsx scripts/seed-unspsc.ts",
    "db:seed:unspsc:remote": "tsx scripts/seed-unspsc.ts --remote",
    "db:seed:items": "tsx scripts/seed-items.ts",
    "db:seed:items:remote": "tsx scripts/seed-items.ts --remote",
    "db:seed:regions": "tsx scripts/seed-regions.ts",
    "db:seed:regions:remote": "tsx scripts/seed-regions.ts --remote",
    "db:seed:municipalities": "tsx scripts/seed-municipalities.ts",
    "db:seed:municipalities:remote": "tsx scripts/seed-municipalities.ts --remote",
    "db:seed:locations": "pnpm run db:seed:regions && pnpm run db:seed:municipalities",
    "db:seed:locations:remote": "pnpm run db:seed:regions:remote && pnpm run db:seed:municipalities:remote",
    "db:seed:suppliers": "tsx scripts/seed-suppliers.ts",
    "db:seed:suppliers:remote": "tsx scripts/seed-suppliers.ts --remote",
    "db:seed:purchases": "tsx scripts/seed-purchases.ts",
    "db:seed:purchases:remote": "tsx scripts/seed-purchases.ts --remote",
    "db:validate:purchases": "tsx scripts/validate-purchase-fks.ts",
    "db:seed:all": "pnpm run db:seed:unspsc && pnpm run db:seed:items && pnpm run db:seed:locations && pnpm run db:seed:suppliers && pnpm run db:seed:purchases",
    "db:seed:all:remote": "pnpm run db:seed:unspsc:remote && pnpm run db:seed:items:remote && pnpm run db:seed:locations:remote && pnpm run db:seed:suppliers:remote && pnpm run db:seed:purchases:remote"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@opennextjs/cloudflare": "^1.13.1",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "d3": "^7.9.0",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.554.0",
    "next": "16.0.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/d3": "^7.4.3",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wrangler": "^4.50.0"
  }
}
